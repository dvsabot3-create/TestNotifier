{
  "name": "dvsa-queen-extension",
  "version": "1.0.0",
  "description": "Advanced DVSA test cancellation finder with stealth technology",
  "main": "background.js",
  "scripts": {
    "build": "webpack --mode=production",
    "build:dev": "webpack --mode=development",
    "watch": "webpack --mode=development --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --ext .js --fix",
    "package": "npm run build && node scripts/package-extension.js",
    "package:dev": "npm run build:dev && node scripts/package-extension.js --dev",
    "deploy": "node scripts/deploy-extension.js",
    "clean": "rimraf dist build",
    "setup": "node scripts/setup-extension.js"
  },
  "keywords": [
    "dvsa",
    "driving-test",
    "cancellation",
    "chrome-extension",
    "stealth",
    "automation"
  ],
  "author": "DVSA Queen Team",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "babel-loader": "^9.1.3",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.8.1",
    "eslint": "^8.50.0",
    "html-webpack-plugin": "^5.5.3",
    "jest": "^29.7.0",
    "mini-css-extract-plugin": "^2.7.6",
    "rimraf": "^5.0.5",
    "style-loader": "^3.3.3",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "archiver": "^6.0.1",
    "chalk": "^4.1.2",
    "fs-extra": "^11.1.1",
    "inquirer": "^8.2.6",
    "node-fetch": "^2.7.0",
    "ora": "^5.4.1",
    "semver": "^7.5.4"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}